{
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",

    "editor.fontFamily": "'JetBrains Mono', 'monospace', monospace",
    "editor.fontLigatures": true,
    "containers.containerClient": "com.microsoft.visualstudio.containers.podman",
    "containers.orchestratorClient": "com.microsoft.visualstudio.orchestrators.podmancompose",
    "redhat.telemetry.enabled": false,
    "docker.extension.enableComposeLanguageServer": false,
    "chatgpt.cliExecutable": "",
    "mesonbuild.downloadLanguageServer": true,
    "comments.openView": "never",
    "window.newWindowProfile": "Default",
    "markdown.extension.completion.enabled": true,
    "markdown.extension.print.theme": "dark",
    "markdown.extension.theming.decoration.renderLink": true,
    "markdown.extension.theming.decoration.renderParagraph": true,
    "mediaPreview.video.loop": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "material-product-icons",
    "workbench.colorTheme": "One Dark Pro Night Flat",
    "workbench.editor.highlightModifiedTabs": true,
    "markdown.preview.fontSize": 12,
    "markdown.preview.lineHeight": 1.4,
    "markdown.extension.theming.decoration.renderHardLineBreak": true,
    "material-icon-theme.activeIconPack": "react_redux",
    "window.customTitleBarVisibility": "windowed",
    "window.menuBarVisibility": "compact",
    "workbench.secondarySideBar.showLabels": false,
    "svelte.enable-ts-plugin": true
}